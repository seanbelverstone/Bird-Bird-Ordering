{
  "name": "bird-bird-ordering",
  "version": "1.0.0",
  "description": "This is an online ordering form for biscuits by the dozen for Bird Bird Biscuit in Austin, TX.",
  "engines": {
    "node": "v13.12.0"
  },
  "main": "server.js",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node -r esm server.js",
    "start:dev": "concurrently \"nodemon -r esm --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "concurrently": "^5.0.0",
    "nodemon": "^1.18.7"
  },
  "dependencies": {
    "@std/esm": "^0.26.0",
    "@stripe/react-stripe-js": "^1.1.2",
    "@stripe/stripe-js": "^1.5.0",
    "availity-reactstrap-validation": "^2.6.1",
    "axios": "^0.19.2",
    "bootstrap": "^4.4.1",
    "date-fns": "^2.9.0",
    "dotenv": "^8.2.0",
    "emailjs-com": "^2.4.1",
    "esm": "^3.2.25",
    "express": "^4.16.3",
    "if-env": "^1.0.4",
    "mysql2": "^2.1.0",
    "node-sass": "^4.14.0",
    "react": "^16.12.0",
    "react-datepicker": "^2.13.0",
    "react-dom": "^16.12.0",
    "react-router-dom": "^5.1.2",
    "react-stripe-elements": "^6.1.2",
    "reactstrap": "^8.4.1",
    "sequelize": "^5.21.5",
    "stripe": "^8.42.0"
  }
}
